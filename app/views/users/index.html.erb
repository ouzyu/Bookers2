<%= render partial: 'books/books_new', locals: { book: @book_new } %>

<h2>Users</h2>

<% @userList.each do |user| %>
<div>

  <p>投稿ユーザー画像: <%= image_tag user.get_profile_image(100,100) %></p>
  <p><%= user.name %></p>
  <p>
    <%= link_to user_path(user.id) do %>
      Show
    <% end %>
  </p>
</div>
<% end %>