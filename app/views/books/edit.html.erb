<h2>Edit Book</h2>

<%= form_with model: @book do |f| %>
  <label for"inputTitle">Title</label>
  <%= f.text_field :title, autofocus: true, id:"inputTitle", placeholder:"Title" %>
  <label for"inputBody">Body</label>
  <%= f.text_field :body, id:"inputBody", placeholder:"Body" %>
  <%= f.submit "Save Edit" %>
<% end %>